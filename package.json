{
  "name": "usmankhwaja.com",
  "version": "1.0.0",
  "description": "personal website made with hugo",
  "main": "index.html",
  "scripts": {
    "build": "rm -rf public && hugo && yarn workbox generateSW workbox.config.js",
    "start": "export HUGO_ENV=development && hugo server",
    "build-functions": "netlify-lambda build assets/lambda/",
    "start-functions": "netlify-lambda serve assets/lambda/",
    "build-sw": "yarn workbox generateSW workbox.config.js"
  },
  "author": "numbfall",
  "repository": "git@github.com:numbfall/usmankhwaja.com.git",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "postcss-cli": "^6.1.3",
    "postcss-combine-duplicated-selectors": "^8.0.3",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-purgecss": "^1.0.0",
    "tachyons": "^4.11.1",
    "workbox-cli": "^4.3.1"
  },
  "eslintConfig": {
    "extends": "google",
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "rules": {
      "max-len": [
        1,
        200,
        4
      ]
    }
  }
}
